  # @return [Boolean]
  #
  # source://minitest//lib/minitest.rb#1140
  def mri?(platform = T.unsafe(nil)); end

  # Is this running on macOS?
  #
  # @return [Boolean]
  #
  # source://minitest//lib/minitest.rb#1147
